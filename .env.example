# Example environment variables for gastric ADCI platform

# Application
ENVIRONMENT=development
DEBUG=true
HOST=0.0.0.0
PORT=8000

# Database
DATABASE_URL=postgresql://user:password@db:5432/adci_db

# Security
SECRET_KEY=your-secret-key

# CORS
CORS_ORIGINS=["http://localhost:3000"]

# Logging
LOG_LEVEL=INFO

# Feature Flags
ENABLE_SWAGGER=true
PATIENT_DATA_ENCRYPTION=false
ENABLE_AUDIT_LOGGING=true

# Deployment
APP_REPLICAS=1
WORKER_REPLICAS=1
API_WORKERS=1
MAX_REQUESTS=500
